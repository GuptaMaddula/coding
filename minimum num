import java.util.*;
class minimum
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
System.out.print("Enter number of elements:");
int n=sc.nextInt();
int arr[]=new int[n];
System.out.print("Enter array elements:");
for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
}
int min=arr[0];
for(int i=1;i<n;i++)
{
if( arr[i]<min)
min=arr[i];
}
System.out.print("Minimum element is:" +min);
}
}
