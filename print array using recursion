import java.util.*;
class arr 
{
public static void main(String[] args) 
{
Scanner sc=new Scanner(System.in);
int n,i;
n=sc.nextInt();
int a[]=new int[n];
for(i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
array(a,0,n);
}
public static void array(int a[], int i, int n)
{
if(i==n)
return;
System.out.print(a[i]+" ");
array(a,i+1,n);
}
}
