import java.util.*;

class vector

{

public static void main(String args[])

{

Vector v=new Vector();

v.add(101);
v.add("Rama");
v.add(6.58);
v.add(102);
v.add("Gita");
v.add(8.98);
System.out.println(v);

v.remove("Gita");
System.out.println(v);
v.set(2,9.17);
System.out.println(v);
if(v.contains("sita"))
System.out.print("true");
else
System.out.print("false");
}
}
