class Solution {
    public int countPrimes(int n) {
        int count,i,s=0;
        for(i=1;i<=n;i++)
        {
            count=0;
            for(int j=1;j<=i;j++)
            {
                if(i%j==0)
                    count=count+1;
            }
            if(count==2)
                s=s+1;
        }
    return s;
        }
}
