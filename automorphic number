import java.util.*;
class automorphic
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("enter n");
int n=sc.nextInt();
int s=n*n;
System.out.println(s);
if(s%100==n||s%10==n)
System.out.print(" It is automorphic num");
else
System.out.print("It is Not automorphic num");
}
}
