import java.util.*;
class singlenum2 
{
public static void main(String args[]) 
{
Scanner sc=new Scanner(System.in);
int size=sc.nextInt();
int nums[]=new int[size];
int res=0;
for(int i=0;i<size;i++)
{
nums[i]=sc.nextInt();
}
int n=nums.length;
for(int i=0;i<n;i++)
{
res=res|nums[i];
}
System.out.print(res);
}
}
