import java.util.*;
class wordslen
{

public static void main(String args[])
{

String str;

Scanner sc=new Scanner(System.in);

int count=0;
System.out.print("Enter string:");

str=sc.nextLine();

String ch[]=str.split(" ");

int n=ch.length;
if(n==0)

System.out.print("0");
else

{
for(int i=0;i<n;i++)
{
String str1=ch[i];

int m=str1.length();

count=count+m;
}
System.out.print(count);
}
}

}
