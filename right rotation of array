import java.util.*;
class rightrotation
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int r=sc.nextInt();
int a[]=new int[n];
int b[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
for(int i=0;i<n;i++)
{
if((i-r)<0)
b[i]=a[n+i-r];
else
b[i]=a[i-r];
}
for(int i=0;i<n;i++)
System.out.print(b[i]+" ");
}
}
